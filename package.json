{
  "name": "sample",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon ./server/index.js  --exec babel-node --presets es2015 react",
    "start": "babel-node ./server/index.js",
    "db:migrate": "sequelize db:migrate --options-path ./server/config/sequelize.json",
    "db:seed:all": "sequelize db:seed:all --options-path ./server/config/sequelize.json",
    "build:server": "babel ./server --out-dir ./build/server",
    "relay": "relay-compiler --src ./client/src/ --schema ./server/graphql/schema.graphql",
    "update:schema": "babel-node ./scripts/update-schema.js",
    "prod": "NODE_ENV=production webpack -p --config webpack.production.config.js",
    "test": "babel-node ./test/index.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.17.2",
    "ejs": "^2.5.6",
    "express": "^4.15.3",
    "express-graphql": "^0.6.6",
    "express-session": "^1.15.3",
    "graphql": "^0.10.4",
    "graphql-relay": "^0.5.2",
    "path": "^0.12.7",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-relay": "^1.1.0",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "relay-runtime": "^1.1.0",
    "sequelize": "^4.3.2",
    "tedious": "^2.0.0",
    "webpack": "^3.3.0",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-relay": "^1.1.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.11.6",
    "relay-compiler": "^1.1.0",
    "watch-less": "0.0.4"
  }
}
